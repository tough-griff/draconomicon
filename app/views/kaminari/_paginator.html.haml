= paginator.render do
  %nav.pagination
    %ul
      - unless current_page.first?
        = first_page_tag
      - unless current_page.first?
        = prev_page_tag
      %li
        %ul
          - each_page do |page|
            - if page.left_outer? || page.right_outer? || page.inside_window?
              = page_tag page
            - elsif !page.was_truncated?
              = gap_tag
      - unless current_page.last?
        = next_page_tag
      - unless current_page.last?
        = last_page_tag
